{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "declaration": true,
    "declarationDir": ".",
    "outDir": ".",
    "rootDir": "src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "src/**.*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "src/SuperJSON.browser.ts"
  ]
}